{
  "name": "bookings-service",
  "version": "1.0.0",
  "description": "Minimal bookings microservice with NestJS",
  "main": "dist/main.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon --watch src --exec ts-node -r tsconfig-paths/register src/main.ts",
    "build": "tsc -p .",
    "test": "jest",
    "test:watch": "jest --watch",
    "migration:run": "node_modules/.bin/typeorm migration:run -d src/data-source.ts"
  },
  "dependencies": {
    "@nestjs/bull": "^0.6.3",
    "@nestjs/bull-workspace": "nestjs/bull",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "@nestjs/websockets": "^11.1.6",
    "bcrypt": "^5.1.0",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ioredis": "^5.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.0",
    "@types/supertest": "^2.0.12",
    "jest": "^29.2.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  }
}
